<body>
<div class="banner-wrap">
<header class="m-header z-img-ready">
<h1 class="m-header__logo" data-jump="home" ><i></i>腾讯课堂</h1>
<ul class="m-header__nav">
<li class="m-header__nav-item"><a class="m-header__nav-link m-header__nav-link--home" href="javascript:void(0)" data-jump="home"><i class="icon-font i-home"></i></a></li>
<li class="m-header__nav-item"><a class="m-header__nav-link m-header__nav-link--center" href="javascript:void(0)" data-jump="center"><i class="icon-font i-center"></i></a></li>
<li class="m-header__nav-item"><a class="m-header__nav-link m-header__nav-link--search" href="javascript:void(0)" data-jump="search"><i class="icon-font i-search"></i></a></li>
</ul>
</header>
<!--搜索框-->
<div class="search-box">
<div class="m-search f-border-1px">
<form id="searchForm" class="u-search f-border-1px">
<i class="icon-font i-search"></i>
<input id="searchInput" class="u-search__input" type="search" maxlength="40" autocomplete="off"
autocorrect="off" placeholder="请输入课程名称">
<div class="u-search__reset-wrap" cmd="resetSearch"><i class="u-search__reset icon-font i-close"></i></div>
</form>
<div class="u-button f-border-1px u-search__btn" cmd="search">搜索</div>
</div>
</div>
<div class="m-search-page"></div><div class="m-search-mask"></div>
<div id="js-nav" class="nav">
<div id="dropdown-mt" class="dropdown" data-target="mt">
<div class="dropdown__description f-icon-font i-v-bottom">全部分类</div>
<div class="dropdown__sep"></div>
</div>
<div id="dropdown-sort" class="dropdown" data-target="sort">
<div class="dropdown__description f-icon-font i-v-bottom">综合排序</div>
</div>
</div>
</div>
<div class="section">
<div class="list-wrapper">
<ul id="js-course-list" class="course-list list-view">
	<% for (var i = 0, l = list.length; i < l; i++) { 
		var it = list[i];
	%>
		<li class="course list-view__item border-bottom">
			<div class="course__cover" style="background-image:url(<%= it.cover %>);">
			</div>
			<h3 class="course__name"><%= it.name %></h3>
			<p class="course__info"><span class="course__info-item u-price<%= it.price ? '' : ' z-free' %>"><%= it.price %></span><span
						class="course__info-item u-message"><i class="icon-font i-user-border"></i><em><%= 12 %></em></span></p>
			<h4 class="course__agency"><%= it.agency.name %></h4>
		</li>
	<% } %>
</ul>
